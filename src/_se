#compdef  se see seee redo rsql re searchl searchle searchlee redosql

function _se(){

    eval "learn_ary=( $(echo "select learning from $ZPWR_SCHEMA_NAME.$ZPWR_TABLE_NAME order by dateAdded" | mysql 2>/dev/null | perl -e '@a=();$c=0;do{chomp;push(@ary,++$c.":".quotemeta($_))}for<>;$c=0;do{print "$_ " if $c++ < 1000}for reverse @ary') )"

    _describe -t zdir 'my learning' learn_ary
}

_se "$@"
