# -*- mode: sh -*-
# vim: set ft=sh:
function zsh-learn-get(){

    local ids learn search
    printf "" > "$ZPWR_TEMPFILE"
    if [[ -z "$1" ]]; then
        getLastItem
        echo "$ZPWR_VARS[item]" >> "$ZPWR_TEMPFILE"
    else
        for search in $@; do
            getItems none
        done >> "$ZPWR_TEMPFILE"
    fi

    print -rz "$(cat "$ZPWR_TEMPFILE")"
    command rm "$ZPWR_TEMPFILE"
}

zsh-learn-get "$@"
